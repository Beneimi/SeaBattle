<Window x:Class="SeaBattle.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeaBattle"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Center" Margin="0,292,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="144" Height="43" TextAlignment="Center" FontWeight="Normal"/>
        <Button x:Name="start" Content="Start" HorizontalAlignment="Center" Margin="0,84,0,0" VerticalAlignment="Top" Height="43" Width="144"/>
        <Button x:Name="highscores" Content="Highscores" HorizontalAlignment="Center" Margin="0,155,0,0" VerticalAlignment="Top" Height="43" Width="144"/>
        <Button x:Name="exit" Content="Exit" HorizontalAlignment="Center" Margin="0,217,0,0" VerticalAlignment="Top" Height="43" Width="144" Click="exit_Click"/>

    </Grid>
</Window>
